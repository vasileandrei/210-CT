def division(number):                                              #1
    """calculates the number of trailing zeros in a factorial"""
    
    if number < 5:                                                 #n
        return 0                                                   #1
    else:                                                          #n-1
        return (number // 5 + division(number//5))                 #n-1
        
Complexity = "O(n)"
print (Complexity)

### Just to verify:
    
def factorial(number):

    if number == 1 or number == 0:
        return 1
    else:
        return (number * factorial(number-1))

#Factorial also O(n)
