def division(number):                                              
    """calculates the number of trailing zeros in a factorial"""
    
    if number < 5:                                                 
        return 0                                                   
    else:                                                          
        return (number // 5 + division(number//5))                 
        
#Complexity = "O(log(n))"

### Just to verify:
    
def factorial(number):

    if number == 1 or number == 0:
        return 1
    else:
        return (number * factorial(number-1))

#Factorial O(n)
