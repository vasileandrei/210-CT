print ("    primeCheck(<number>, sq(<number>)")

"""This function checks if the number is prime"""

from math import sqrt as sq

#it is important that in the END section, SQ(N) in inputed
# calculating the sqrt of the nubmer will provide a lower complexity

def primeCheck(number, end, value=2):
    if value <= end:
        if number % value == 0:
            return False
        else:
            return primeCheck(number, end, value+1)            
    return True

#Complexity: O(sqrt(n))
    
    
